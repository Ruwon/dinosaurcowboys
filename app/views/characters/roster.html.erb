<div class="panel">
  <h1>Roster</h1>

  <h2>Core</h2>
  <ul>
    <% @core.each do |character| %>
    <li><%= link_to character.name, user_character_path(character.user, character) %></li>
    <% end %>
  </ul>

  <h2>Applicants</h2>
  <ul>
    <% @applicants.each do |character| %>
    <li><%= character.name %></li>
    <% end %>
  </ul>
</div>
